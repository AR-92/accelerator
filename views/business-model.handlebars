<!-- Business Model Page -->
<div class="min-h-screen bg-background">
<div id="business-model-main-content" class="max-w-4xl mx-auto px-4 sm:px-6 lg:px-8 py-6 w-full">
        <div class="flex flex-col gap-4 lg:gap-6">
  <div class="flex items-center justify-between">
    <div class="grid gap-1">
      <h1 class="font-heading text-2xl font-semibold text-foreground">Business Model Canvas</h1>
      <p class="text-base text-muted-foreground">Design and visualize your business model.</p>
    </div>
  </div>

    <!-- Canvas Grid -->
    <div class="grid grid-cols-1 gap-4 md:grid-cols-5">
      <!-- Key Partners -->
      <div class="md:col-span-2 rounded-lg bg-card p-4 text-card-foreground">
        <h3 class="font-medium text-foreground mb-3 flex items-center gap-2">
{{{icon 'users' class='w-4 h-4 text-primary')}}}
          Key Partners
        </h3>
        <div class="space-y-2">
          <div class="p-2 bg-primary/10 rounded text-sm">Technology providers</div>
          <div class="p-2 bg-primary/10 rounded text-sm">Strategic alliances</div>
          <div class="p-2 bg-primary/10 rounded text-sm">Distribution partners</div>
        </div>
      </div>

      <!-- Key Activities -->
      <div class="rounded-lg bg-card p-4 text-card-foreground">
        <h3 class="font-medium text-foreground mb-3 flex items-center gap-2">
{{{icon 'activity' class='w-4 h-4 text-secondary')}}}
          Key Activities
        </h3>
        <div class="space-y-2">
          <div class="p-2 bg-secondary/10 rounded text-sm">Platform development</div>
          <div class="p-2 bg-secondary/10 rounded text-sm">Customer acquisition</div>
          <div class="p-2 bg-secondary/10 rounded text-sm">Market expansion</div>
        </div>
      </div>

      <!-- Value Propositions -->
      <div class="rounded-lg bg-card p-4 text-card-foreground">
        <h3 class="font-medium text-foreground mb-3 flex items-center gap-2">
{{{icon 'star' class='w-4 h-4 text-success')}}}
          Value Propositions
        </h3>
        <div class="space-y-2">
          <div class="p-2 bg-success/10 rounded text-sm">Time savings</div>
          <div class="p-2 bg-success/10 rounded text-sm">Cost reduction</div>
          <div class="p-2 bg-success/10 rounded text-sm">Quality improvement</div>
        </div>
      </div>

      <!-- Customer Relationships -->
      <div class="rounded-lg bg-card p-4 text-card-foreground">
        <h3 class="font-medium text-foreground mb-3 flex items-center gap-2">
{{{icon 'heart' class='w-4 h-4 text-warning')}}}
          Customer Relationships
        </h3>
        <div class="space-y-2">
          <div class="p-2 bg-warning/10 rounded text-sm">Self-service platform</div>
          <div class="p-2 bg-warning/10 rounded text-sm">Community support</div>
          <div class="p-2 bg-warning/10 rounded text-sm">Personal assistance</div>
        </div>
      </div>

      <!-- Customer Segments -->
      <div class="md:col-span-2 rounded-lg bg-card p-4 text-card-foreground">
        <h3 class="font-medium text-foreground mb-3 flex items-center gap-2">
{{{icon 'user-check' class='w-4 h-4 text-primary')}}}
          Customer Segments
        </h3>
        <div class="space-y-2">
          <div class="p-2 bg-primary/10 rounded text-sm">Small businesses</div>
          <div class="p-2 bg-primary/10 rounded text-sm">Startups</div>
          <div class="p-2 bg-primary/10 rounded text-sm">Enterprise clients</div>
        </div>
      </div>

      <!-- Channels -->
      <div class="rounded-lg bg-card p-4 text-card-foreground">
        <h3 class="font-medium text-foreground mb-3 flex items-center gap-2">
{{{icon 'send' class='w-4 h-4 text-secondary')}}}
          Channels
        </h3>
        <div class="space-y-2">
          <div class="p-2 bg-secondary/10 rounded text-sm">Direct sales</div>
          <div class="p-2 bg-secondary/10 rounded text-sm">Online platform</div>
          <div class="p-2 bg-secondary/10 rounded text-sm">Partnerships</div>
        </div>
      </div>

      <!-- Cost Structure -->
      <div class="md:col-span-2 rounded-lg bg-card p-4 text-card-foreground">
        <h3 class="font-medium text-foreground mb-3 flex items-center gap-2">
{{{icon 'dollar-sign' class='w-4 h-4 text-destructive')}}}
          Cost Structure
        </h3>
        <div class="space-y-2">
          <div class="p-2 bg-destructive/10 rounded text-sm">Development costs</div>
          <div class="p-2 bg-destructive/10 rounded text-sm">Marketing expenses</div>
          <div class="p-2 bg-destructive/10 rounded text-sm">Operational costs</div>
        </div>
      </div>

      <!-- Revenue Streams -->
      <div class="md:col-span-2 rounded-lg bg-card p-4 text-card-foreground">
        <h3 class="font-medium text-foreground mb-3 flex items-center gap-2">
{{{icon 'trending-up' class='w-4 h-4 text-success')}}}
          Revenue Streams
        </h3>
        <div class="space-y-2">
          <div class="p-2 bg-success/10 rounded text-sm">Subscription fees</div>
          <div class="p-2 bg-success/10 rounded text-sm">Premium features</div>
          <div class="p-2 bg-success/10 rounded text-sm">Consulting services</div>
        </div>
      </div>
    </div>

    <!-- Actions -->
    <div class="flex gap-4">
      <button class="px-4 py-2 bg-primary text-primary-foreground rounded-md hover:bg-primary/90 transition-colors">
{{{icon 'edit' class='w-4 h-4 mr-2')}}}
        Edit Canvas
      </button>
      <button class="px-4 py-2 border border-border text-foreground rounded-md hover:bg-muted transition-colors">
{{{icon 'download' class='w-4 h-4 mr-2')}}}
        Export
      </button>
    </div>
  </div>
</div>
</div>