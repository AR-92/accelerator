<!-- Dashboard Overview Page -->
<div class="min-h-screen bg-background">
<div id="dashboard-main-content" class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8 py-6 w-full">
                <div class="flex flex-col gap-4 lg:gap-6">
  <div class="flex items-center justify-between">
    <div class="grid gap-1">
      <h1 class="font-heading text-2xl font-semibold text-foreground">Dashboard
        Overview</h1>
      <p class="text-base text-muted-foreground">Monitor your startup's progress
        and key metrics at a glance.</p>
    </div>
  </div>

    <!-- Key Metrics -->
    <div class="grid grid-cols-1 gap-4 sm:grid-cols-2 lg:grid-cols-4 xl:grid-cols-5">
      <div class="rounded-lg bg-card px-4 py-6 text-card-foreground shadow-sm">
        <div class="flex items-center justify-between px-3 py-1">
          <div class="flex flex-col gap-1">
             <h3 class="text-sm font-medium text-muted-foreground">Total Projects</h3>
             <p class="text-xl font-semibold text-foreground">12</p>
          </div>
           <div>
              <div class="bg-primary/20 rounded-md">
                <div class="w-10 h-10 bg-primary/20 rounded-full relative">
                  <div class="flex items-center justify-center w-9 h-9 bg-primary rounded-full absolute top-0.5 left-0.5">
{{{icon 'folder' class='w-4 h-4 text-white'}}}
</div>
                </div>
              </div>
           </div>
        </div>
        <p class="flex items-center gap-1 text-sm font-medium text-muted-foreground">

{{{icon 'trending-up' class='w-5 h-4'}}}

          <span class="text-green-600 dark:text-green-400">+3</span>Since last
          month
        </p>
      </div>

      <div class="rounded-lg bg-card px-4 py-6 text-card-foreground shadow-sm">
        <div class="flex items-center justify-between px-3 py-1">
          <div class="flex flex-col gap-1">
            <h3 class="text-sm font-medium text-muted-foreground">Active Ideas</h3>
            <p class="text-xl font-semibold text-foreground">28</p>
          </div>
          <div>
              <div class="bg-secondary/20 rounded-md">
                <div class="w-10 h-10 bg-secondary/20 rounded-full relative">
                  <div class="flex items-center justify-center w-9 h-9 bg-secondary rounded-full absolute top-0.5 left-0.5">
{{{icon 'lightbulb' class='w-4 h-4 text-white'}}}
</div>
               </div>
             </div>
          </div>
        </div>
        <p class="flex items-center gap-1 text-sm font-medium text-muted-foreground">

{{{icon 'trending-up' class='w-5 h-4'}}}

          <span class="text-green-600 dark:text-green-400">+7</span>Since last
          month
        </p>
      </div>

      <div class="rounded-lg bg-card px-4 py-6 text-card-foreground shadow-sm">
        <div class="flex items-center justify-between px-3 py-1">
          <div class="flex flex-col gap-1">
            <h3 class="text-sm font-medium text-muted-foreground">Team Members</h3>
            <p class="text-xl font-semibold text-foreground">8</p>
          </div>
          <div>
              <div class="bg-primary/20 rounded-md">
                <div class="w-10 h-10 bg-primary/20 rounded-full relative">
                  <div class="flex items-center justify-center w-9 h-9 bg-primary rounded-full absolute top-0.5 left-0.5">
{{{icon 'users' class='w-4 h-4 text-white'}}}
</div>
               </div>
             </div>
          </div>
        </div>
        <p class="flex items-center gap-1 text-sm font-medium text-muted-foreground">

{{{icon 'trending-up' class='w-5 h-4'}}}

          <span class="text-green-600 dark:text-green-400">+2</span>Since last
          month
        </p>
      </div>

      <div class="rounded-lg bg-card px-4 py-6 text-card-foreground shadow-sm">
        <div class="flex items-center justify-between px-3 py-1">
          <div class="flex flex-col gap-1">
            <h3 class="text-sm font-medium text-muted-foreground">Revenue</h3>
            <p class="text-xl font-semibold text-foreground">$45K</p>
          </div>
          <div>
              <div class="bg-success/20 rounded-md">
                <div class="w-10 h-10 bg-success/20 rounded-full relative">
                  <div class="flex items-center justify-center w-9 h-9 bg-success rounded-full absolute top-0.5 left-0.5">
{{{icon 'dollar-sign' class='w-4 h-4 text-white'}}}
</div>
               </div>
             </div>
          </div>
        </div>
        <p class="flex items-center gap-1 text-sm font-medium text-muted-foreground">

{{{icon 'trending-up' class='w-5 h-4'}}}

          <span class="text-green-600 dark:text-green-400">+12%</span>Since last
          month
        </p>
      </div>

      <div class="rounded-lg bg-card px-4 py-6 text-card-foreground shadow-sm">
        <div class="flex items-center justify-between px-3 py-1">
          <div class="flex flex-col gap-1">
            <h3 class="text-sm font-medium text-muted-foreground">Funding Raised</h3>
            <p class="text-xl font-semibold text-foreground">$250K</p>
          </div>
          <div>
              <div class="bg-primary/20 rounded-md">
                <div class="w-10 h-10 bg-primary/20 rounded-full relative">
                  <div class="flex items-center justify-center w-9 h-9 bg-primary rounded-full absolute top-0.5 left-0.5">
{{{icon 'trending-up' class='w-4 h-4 text-white'}}}
</div>
               </div>
             </div>
          </div>
        </div>
        <p class="flex items-center gap-1 text-sm font-medium text-muted-foreground">

{{{icon 'trending-up' class='w-5 h-4'}}}

          <span class="text-green-600 dark:text-green-400">+25%</span>Since last
          month
        </p>
      </div>
    </div>

    <!-- Progress Overview -->
    <div class="flex flex-col items-stretch gap-4 lg:flex-row">
      <div class="w-full rounded-lg bg-card px-6 py-5 text-card-foreground lg:w-[60%] lg:min-w-[60%]">
        <h3 class="text-lg font-medium text-foreground mb-4">Overall Progress</h3>
        <div class="grid grid-cols-2 gap-4">
          <div class="p-4 bg-success/10 rounded-lg border border-border">
            <div class="flex items-center gap-3 mb-2">
              <div class="w-8 h-8 bg-success/20 rounded-lg flex items-center justify-center">
{{{icon 'check' class='w-4 h-4 text-success'}}}
              </div>
              <h4 class="font-medium text-success">Idea Validation</h4>
            </div>
            <p class="text-sm text-success/80">Completed successfully</p>
          </div>

          <div class="p-4 bg-warning/10 rounded-lg border border-border">
            <div class="flex items-center gap-3 mb-2">
              <div class="w-8 h-8 bg-warning/20 rounded-lg flex items-center justify-center">
{{{icon 'clock' class='w-4 h-4 text-warning'}}}
              </div>
              <h4 class="font-medium text-warning">Prototype Development</h4>
            </div>
            <p class="text-sm text-warning/80">In progress</p>
          </div>

          <div class="p-4 bg-primary/10 rounded-lg border border-border">
            <div class="flex items-center gap-3 mb-2">
              <div class="w-8 h-8 bg-primary/20 rounded-lg flex items-center justify-center">
{{{icon 'bar-chart' class='w-4 h-4 text-primary'}}}
              </div>
              <h4 class="font-medium text-primary">Market Research</h4>
            </div>
            <p class="text-sm text-primary/80">Data collected</p>
          </div>

          <div class="p-4 bg-destructive/10 rounded-lg border border-border">
            <div class="flex items-center gap-3 mb-2">
              <div class="w-8 h-8 bg-destructive/20 rounded-lg flex items-center justify-center">
{{{icon 'x' class='w-4 h-4 text-destructive'}}}
              </div>
              <h4 class="font-medium text-destructive">Launch Preparation</h4>
            </div>
            <p class="text-sm text-destructive/80">Planning phase</p>
          </div>
        </div>
      </div>

      <div class="w-full rounded-lg bg-card px-6 py-5 text-card-foreground">
        <h3 class="text-lg font-medium text-foreground mb-4">Quick Actions</h3>
        <div class="space-y-3">
          {{> button text="Generate New Idea" icon="plus" variant="secondary" href="/dashboard/idea" class="flex items-center justify-center w-full px-4 py-3 rounded-md text-sm font-medium transition"}}
          {{> button text="View Analytics" icon="bar-chart" variant="primary" class="flex items-center justify-center w-full px-4 py-3 rounded-md text-sm font-medium transition"}}
          {{> button text="Manage Team" icon="users" variant="secondary" class="flex items-center justify-center w-full px-4 py-3 rounded-md text-sm font-medium transition"}}
        </div>
      </div>
    </div>

    <!-- Recent Activities -->
    <div class="rounded-lg bg-card px-4 py-5 text-card-foreground">
      <div class="flex flex-col items-center justify-between gap-4 lg:flex-row">
        <h3 class="text-xl font-semibold text-primary">Recent Activities</h3>
      </div>

       <div class="mt-7 w-full rounded-lg">
        <div class="relative w-full overflow-auto">
          <table class="w-full caption-bottom text-sm min-w-full overflow-x-auto">
            <thead class="[&_tr]:border-b h-16 bg-muted">
              <tr class="border-b transition-colors hover:bg-muted/50 data-[state=selected]:bg-muted">
                <th class="h-12 px-4 text-left align-middle [&:has([role=checkbox])]:pr-0 text-base font-semibold text-primary">Activity</th>
                <th class="h-12 px-4 text-left align-middle [&:has([role=checkbox])]:pr-0 text-base font-semibold text-primary">User</th>
                <th class="h-12 px-4 text-left align-middle [&:has([role=checkbox])]:pr-0 text-base font-semibold text-primary">Type</th>
                <th class="h-12 px-4 text-left align-middle [&:has([role=checkbox])]:pr-0 text-base font-semibold text-primary">Date</th>
              </tr>
            </thead>
            <tbody class="[&_tr:last-child]:border-0">
              <tr class="border-b transition-colors hover:bg-muted/50 data-[state=selected]:bg-muted">
                <td class="p-4 align-middle [&:has([role=checkbox])]:pr-0">
                  <div class="flex items-center gap-3">
                    <div class="w-10 h-10 bg-primary/20 rounded-lg flex items-center justify-center">
{{{icon 'plus' class='w-5 h-5 text-primary'}}}
                    </div>
                    <p class="text-sm font-medium text-foreground">Created new project</p>
                  </div>
                </td>
                <td class="p-4 align-middle [&:has([role=checkbox])]:pr-0">
                  <span class="px-2 py-1 bg-primary/20 text-primary text-xs rounded">John Doe</span>
                </td>
                <td class="p-4 align-middle [&:has([role=checkbox])]:pr-0">
                  <span class="px-2 py-1 bg-success/20 text-success text-xs rounded">Project</span>
                </td>
                <td class="p-4 align-middle [&:has([role=checkbox])]:pr-0">
                  <p class="text-sm font-medium text-foreground">2 hours ago</p>
                </td>
              </tr>

              <tr class="border-b transition-colors hover:bg-muted/50 data-[state=selected]:bg-muted">
                <td class="p-4 align-middle [&:has([role=checkbox])]:pr-0">
                  <div class="flex items-center gap-3">
                    <div class="w-10 h-10 bg-warning/20 rounded-lg flex items-center justify-center">
{{{icon 'lightbulb' class='w-5 h-5 text-warning'}}}
                    </div>
                    <p class="text-sm font-medium text-foreground">Submitted idea</p>
                  </div>
                </td>
                <td class="p-4 align-middle [&:has([role=checkbox])]:pr-0">
                  <span class="px-2 py-1 bg-warning/20 text-warning text-xs rounded">Sarah Smith</span>
                </td>
                <td class="p-4 align-middle [&:has([role=checkbox])]:pr-0">
                  <span class="px-2 py-1 bg-primary/20 text-primary text-xs rounded">Idea</span>
                </td>
                <td class="p-4 align-middle [&:has([role=checkbox])]:pr-0">
                  <p class="text-sm font-medium text-foreground">4 hours ago</p>
                </td>
              </tr>

              <tr class="border-b transition-colors hover:bg-muted/50 data-[state=selected]:bg-muted">
                <td class="p-4 align-middle [&:has([role=checkbox])]:pr-0">
                  <div class="flex items-center gap-3">
                    <div class="w-10 h-10 bg-destructive/20 rounded-lg flex items-center justify-center">
{{{icon 'dollar-sign' class='w-5 h-5 text-destructive'}}}
                    </div>
                    <p class="text-sm font-medium text-foreground">Updated financials</p>
                  </div>
                </td>
                <td class="p-4 align-middle [&:has([role=checkbox])]:pr-0">
                  <span class="px-2 py-1 bg-destructive/20 text-destructive text-xs rounded">Mike Johnson</span>
                </td>
                <td class="p-4 align-middle [&:has([role=checkbox])]:pr-0">
                  <span class="px-2 py-1 bg-primary/20 text-primary text-xs rounded">Finance</span>
                </td>
                <td class="p-4 align-middle [&:has([role=checkbox])]:pr-0">
                  <p class="text-sm font-medium text-foreground">6 hours ago</p>
                </td>
              </tr>
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
</div>
</div>