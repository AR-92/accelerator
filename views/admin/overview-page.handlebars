<!-- Generic Overview Page Template -->
<div class="min-h-screen bg-background">
    <div id="{{section}}-main-content" class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8 py-6 w-full">
                <div class="flex flex-col gap-4 lg:gap-6">
  <div class="flex items-center justify-between">
    <div class="grid gap-1">
      <h1 class="font-heading text-2xl font-semibold text-foreground">Startup
        Overview</h1>
      <p class="text-base text-muted-foreground">Monitor your startup's progress
        and key metrics.</p>
    </div>
    <div class="flex items-center gap-2">
      <button class="flex items-center gap-2 px-4 py-2 bg-primary/10 text-primary rounded-md hover:bg-primary/20">
        {{{icon 'plus' size='16'}}}
        New Project
      </button>
    </div>
  </div>

   <!-- Startup Metrics -->
   <div class="grid grid-cols-1 gap-4 sm:grid-cols-2 lg:grid-cols-4 xl:grid-cols-5">
     <div class="rounded-lg bg-card px-4 py-6 text-card-foreground shadow-sm">
       <div class="flex items-center justify-between px-3 py-1">
         <div class="flex flex-col gap-1">
           <h3 class="text-sm font-medium text-muted-foreground">Active Projects</h3>
           <p class="text-xl font-semibold text-foreground">12</p>
         </div>
         <div>
           <div class="bg-primary/20 rounded-md">
             <div class="w-10 h-10 bg-primary/20 rounded-full relative">
               <div class="flex items-center justify-center w-9 h-9 bg-primary rounded-full absolute top-0.5 left-0.5">
 {{{icon 'folder-open' class='w-4 h-4 text-primary-foreground'}}}
 </div>
             </div>
           </div>
         </div>
       </div>
       <p class="flex items-center gap-1 text-sm font-medium text-muted-foreground">

 {{{icon 'trending-up' class='w-5 h-4'}}}

         <span class="text-primary">+2</span>Since last
         month
       </p>
     </div>

     <div class="rounded-lg bg-card px-4 py-6 text-card-foreground shadow-sm">
       <div class="flex items-center justify-between px-3 py-1">
         <div class="flex flex-col gap-1">
           <h3 class="text-sm font-medium text-muted-foreground">Total Ideas</h3>
           <p class="text-xl font-semibold text-foreground">47</p>
         </div>
         <div>
           <div class="bg-secondary/20 rounded-md">
             <div class="w-10 h-10 bg-secondary/20 rounded-full relative">
               <div class="flex items-center justify-center w-9 h-9 bg-secondary rounded-full absolute top-0.5 left-0.5">
 {{{icon 'lightbulb' class='w-4 h-4 text-secondary-foreground'}}}
 </div>
             </div>
           </div>
         </div>
       </div>
       <p class="flex items-center gap-1 text-sm font-medium text-muted-foreground">

 {{{icon 'trending-up' class='w-5 h-4'}}}

         <span class="text-secondary">+8</span>Since last
         month
       </p>
     </div>

     <div class="rounded-lg bg-card px-4 py-6 text-card-foreground shadow-sm">
       <div class="flex items-center justify-between px-3 py-1">
         <div class="flex flex-col gap-1">
           <h3 class="text-sm font-medium text-muted-foreground">Team Members</h3>
           <p class="text-xl font-semibold text-foreground">15</p>
         </div>
         <div>
           <div class="bg-primary/20 rounded-md">
             <div class="w-10 h-10 bg-primary/20 rounded-full relative">
               <div class="flex items-center justify-center w-9 h-9 bg-primary rounded-full absolute top-0.5 left-0.5">
 {{{icon 'users' class='w-4 h-4 text-primary-foreground'}}}
 </div>
             </div>
           </div>
         </div>
       </div>
       <p class="flex items-center gap-1 text-sm font-medium text-muted-foreground">

 {{{icon 'trending-up' class='w-5 h-4'}}}

         <span class="text-primary">+3</span>Since last
         month
       </p>
     </div>

     <div class="rounded-lg bg-card px-4 py-6 text-card-foreground shadow-sm">
       <div class="flex items-center justify-between px-3 py-1">
         <div class="flex flex-col gap-1">
           <h3 class="text-sm font-medium text-muted-foreground">Funding Raised</h3>
           <p class="text-xl font-semibold text-foreground">$125K</p>
         </div>
         <div>
           <div class="bg-success/20 rounded-md">
             <div class="w-10 h-10 bg-success/20 rounded-full relative">
               <div class="flex items-center justify-center w-9 h-9 bg-success rounded-full absolute top-0.5 left-0.5">
 {{{icon 'dollar-sign' class='w-4 h-4 text-success-foreground'}}}
 </div>
             </div>
           </div>
         </div>
       </div>
       <p class="flex items-center gap-1 text-sm font-medium text-muted-foreground">

 {{{icon 'trending-up' class='w-5 h-4'}}}

         <span class="text-success">+25%</span>Since last
         month
       </p>
     </div>

     <div class="rounded-lg bg-card px-4 py-6 text-card-foreground shadow-sm">
       <div class="flex items-center justify-between px-3 py-1">
         <div class="flex flex-col gap-1">
           <h3 class="text-sm font-medium text-muted-foreground">Growth Rate</h3>
           <p class="text-xl font-semibold text-foreground">18%</p>
         </div>
         <div>
           <div class="bg-primary/20 rounded-md">
             <div class="w-10 h-10 bg-primary/20 rounded-full relative">
               <div class="flex items-center justify-center w-9 h-9 bg-primary rounded-full absolute top-0.5 left-0.5">
 {{{icon 'trending-up' class='w-4 h-4 text-primary-foreground'}}}
 </div>
             </div>
           </div>
         </div>
       </div>
       <p class="flex items-center gap-1 text-sm font-medium text-muted-foreground">

 {{{icon 'trending-up' class='w-5 h-4'}}}

         <span class="text-primary">+5%</span>Since last
         month
       </p>
     </div>
  </div>
</div>
             </div>

