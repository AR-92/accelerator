  async function autoFillForm(formType) {
    try {
      const response = await fetch('/api/ai/generate-form', {
        method: 'POST',
        headers: {
          'Content-Type': 'application/json',
        },
        body: JSON.stringify({
          formType: formType,
          context: 'idea-model' // Add more context if needed
        })
      });

      const data = await response.json();

      if (data.success) {
        fillFormWithData(formType, data.generatedContent);
        alert('Form auto-filled with AI-generated content. Please review and edit as needed.');
      } else {
        alert('Error generating content: ' + data.error);
      }
    } catch (error) {
      console.error('Auto-fill error:', error);
      alert('Failed to auto-fill form. Please try again.');
    }
  }

