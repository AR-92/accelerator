    // Initialize enterprise approval functionality
    function initEnterpriseApproval() {
      // Handle approve enterprise buttons
      document.querySelectorAll('.approve-enterprise-btn').forEach(button => {
        button.addEventListener('click', function() {
          const enterpriseId = this.dataset.enterpriseId;
          const enterpriseCard = this.closest('.border');

          if (confirm('Are you sure you want to approve this enterprise?')) {
            console.log(`Approving enterprise: ${enterpriseId}`);
            // In a real implementation, this would make an API call
            Toast.success('Enterprise approved successfully!');
            enterpriseCard.remove();
            updatePendingCount();
          }
        });
      });

      // Handle reject enterprise buttons
      document.querySelectorAll('.reject-enterprise-btn').forEach(button => {
        button.addEventListener('click', function() {
          const enterpriseId = this.dataset.enterpriseId;
          const enterpriseCard = this.closest('.border');

          const reason = prompt('Please provide a reason for rejection:');
          if (reason !== null && reason.trim() !== '') {
            console.log(`Rejecting enterprise: ${enterpriseId}, Reason: ${reason}`);
            // In a real implementation, this would make an API call
            Toast.error('Enterprise rejected.');
            enterpriseCard.remove();
            updatePendingCount();
          }
        });
      });
    }

    // Update pending approvals count
    function updatePendingCount() {
      const remainingCards = document.querySelectorAll('.approve-enterprise-btn').length;
      const countBadge = document.querySelector('.bg-yellow-100');
      if (countBadge) {
        countBadge.textContent = `${remainingCards} Pending`;
      }

      // Hide the section if no pending approvals
      const approvalSection = document.querySelector('.space-y-4');
      if (remainingCards === 0 && approvalSection) {
        approvalSection.innerHTML = `
          <div class='text-center py-8'>
            <svg xmlns='http://www.w3.org/2000/svg' width='48' height='48' viewBox='0 0 24 24' fill='none' stroke='currentColor' stroke-width='1' stroke-linecap='round' stroke-linejoin='round' class='lucide lucide-check-circle mx-auto text-green-500 mb-4'>
