   // Initialize permissions modal
   function initPermissionsModal() {
     // Handle edit permissions buttons
     document.querySelectorAll('.edit-permissions-btn').forEach(button => {
       button.addEventListener('click', function() {
         const userId = this.dataset.userId;
         const userRow = document.querySelector(`tr[data-user-id="${userId}"]`);
         const userName = userRow.querySelector('td:first-child .font-medium').textContent;
         const userRole = userRow.querySelector('.role-select').value;
         
         document.getElementById('modal-user-name').textContent = userName;
         document.getElementById('modal-user-role').value = userRole;
         
         // In a real implementation, we would load the user's permissions here
         // For now, just check some sample permissions based on role
         const permissionsChecklist = document.getElementById('permissions-checklist');
         const checkboxes = permissionsChecklist.querySelectorAll('input[type="checkbox"]');
         checkboxes.forEach(checkbox => {
           // Reset all
           checkbox.checked = false;
           
           // Set based on role
           if (userRole === 'Executive') {
             checkbox.checked = true;
           } else if (userRole === 'Board Member') {
             if (checkbox.value === 'read' || checkbox.value === 'write') {
               checkbox.checked = true;
             }
           } else if (userRole === 'Manager') {
             if (checkbox.value === 'read' || checkbox.value === 'write') {
               checkbox.checked = true;
             }
           } else if (userRole === 'Editor') {
             if (checkbox.value === 'read') {
               checkbox.checked = true;
             }
           }
         });
         
         document.getElementById('permissions-modal').classList.remove('hidden');
       });
     });
     
     // Close permissions modal
     document.getElementById('close-permissions-modal').addEventListener('click', function() {
       document.getElementById('permissions-modal').classList.add('hidden');
     });
     
     document.getElementById('cancel-permissions').addEventListener('click', function() {
       document.getElementById('permissions-modal').classList.add('hidden');
     });
     
     // Handle save permissions
     document.getElementById('save-permissions').addEventListener('click', function() {
       const userId = document.querySelector('tr[data-user-id]').dataset.userId; // This would be the current user being edited
       const newRole = document.getElementById('modal-user-role').value;
       const permissions = Array.from(document.querySelectorAll('#permissions-checklist input:checked'))
         .map(checkbox => checkbox.value);
       
       console.log(`Updating user ${userId} to role ${newRole} with permissions:`, permissions);
       
       // In a real implementation, this would update the user's permissions
       // For now, just show a success message and close the modal
        Toast.success(`Permissions updated for user. Role: ${newRole}, Permissions: ${permissions.join(', ')}`);
       document.getElementById('permissions-modal').classList.add('hidden');
     });
   }

