<div class='p-4 w-full'>
  <div class='flex flex-col gap-4 lg:gap-6'>
    <div class='flex items-center justify-between'>
      <div class='grid gap-1'>
        <h1 class='font-heading text-2xl font-semibold dark:text-white'>Enterprise
          Dashboard</h1>
        <p class='text-base text-muted-foreground'>Manage multiple startups and their ideas across your enterprise.</p>
      </div>
      <div class='flex items-center gap-2'>
        <button class='flex items-center gap-2 px-4 py-2 bg-primary text-primary-foreground rounded-md hover:bg-primary/90'>
          <svg xmlns='http://www.w3.org/2000/svg' width='16' height='16' viewBox='0 0 24 24' fill='none' stroke='currentColor' stroke-width='2' stroke-linecap='round' stroke-linejoin='round' class='lucide lucide-plus'>
            <path d='M5 12h14'/>
            <path d='M12 5v14'/>
          </svg>
          New Startup
        </button>
      </div>
    </div>

    <!-- Enterprise Metrics -->
    <div class='grid grid-cols-1 gap-[17px] sm:grid-cols-2 lg:grid-cols-4 xl:grid-cols-5'>
      <div
        class='rounded-[11px] bg-card px-[16px] py-[20px] text-card-foreground shadow-sm'
        style='box-shadow: rgba(0, 0, 0, 0.15) 0px 1px 2px 0px;'
      >
        <div class='flex items-center justify-between px-[10px] py-[4px]'>
          <div class='flex flex-col gap-[4px]'>
            <h3
              class='text-[14px] font-[500] text-gray-500 dark:text-gray-400'
            >Total Startups</h3>
            <p
              class='text-[18px] font-[600] text-gray-900 dark:text-gray-100'
            >{{startupCount}}</p>
          </div>
          <div>
            <div class='w-11 h-11 bg-blue-100 rounded-lg flex items-center justify-center'>{{lucide 'Building2' 'w-5 h-5 fill-blue-600'}}</div>
          </div>
        </div>
        <p
          class='flex items-center gap-[4px] text-[13px] font-[500] text-gray-500 dark:text-gray-400'
        >
          <svg
            xmlns='http://www.w3.org/2000/svg'
            width='24'
            height='20'
            viewBox='0 0 24 20'
            fill='none'
          >
            <path
              d='M7.02979 8.30925H11.0298V15.7426L13.0398 15.7676V8.30925H17.0298L12.0298 4.14258L7.02979 8.30925Z'
              fill='#2196F3'
            ></path>
          </svg>
          <span class='text-[#2196F3] dark:text-blue-400'>+3</span>Since last
          month
        </p>
      </div>

      <div
        class='rounded-[11px] bg-card px-[16px] py-[20px] text-card-foreground shadow-sm'
        style='box-shadow: rgba(0, 0, 0, 0.15) 0px 1px 2px 0px;'
      >
        <div class='flex items-center justify-between px-[10px] py-[4px]'>
          <div class='flex flex-col gap-[4px]'>
            <h3
              class='text-[14px] font-[500] text-gray-500 dark:text-gray-400'
            >Total Ideas</h3>
            <p
              class='text-[18px] font-[600] text-gray-900 dark:text-gray-100'
            >{{ideaCount}}</p>
          </div>
          <div>
            <div class='w-11 h-11 bg-yellow-50 rounded-lg flex items-center justify-center'>{{lucide 'Lightbulb' 'w-5 h-5 fill-orange-500'}}</div>
          </div>
        </div>
        <p
          class='flex items-center gap-[4px] text-[13px] font-[500] text-gray-500 dark:text-gray-400'
        >
          <svg
            xmlns='http://www.w3.org/2000/svg'
            width='24'
            height='20'
            viewBox='0 0 24 20'
            fill='none'
          >
            <path
              d='M7.02979 8.30925H11.0298V15.7426L13.0398 15.7676V8.30925H17.0298L12.0298 4.14258L7.02979 8.30925Z'
              fill='#FF9800'
            ></path>
          </svg>
          <span class='text-[#FF9800] dark:text-yellow-400'>+12</span>Since
          last month
        </p>
      </div>

      <div
        class='rounded-[11px] bg-card px-[16px] py-[20px] text-card-foreground shadow-sm'
        style='box-shadow: rgba(0, 0, 0, 0.15) 0px 1px 2px 0px;'
      >
        <div class='flex items-center justify-between px-[10px] py-[4px]'>
          <div class='flex flex-col gap-[4px]'>
            <h3
              class='text-[14px] font-[500] text-gray-500 dark:text-gray-400'
            >Active Projects</h3>
            <p
              class='text-[18px] font-[600] text-gray-900 dark:text-gray-100'
            >{{activeProjectCount}}</p>
          </div>
          <div>
            <div class='w-11 h-11 bg-red-50 rounded-lg flex items-center justify-center'>{{lucide 'Activity' 'w-5 h-5 fill-pink-500'}}</div>
          </div>
        </div>
        <p
          class='flex items-center gap-[4px] text-[13px] font-[500] text-gray-500 dark:text-gray-400'
        >
          <svg
            xmlns='http://www.w3.org/2000/svg'
            width='24'
            height='20'
            viewBox='0 0 24 20'
            fill='none'
          >
            <path
              d='M7.02979 8.30925H11.0298V15.7426L13.0398 15.7676V8.30925H17.0298L12.0298 4.14258L7.02979 8.30925Z'
              fill='#E91E63'
            ></path>
          </svg>
          <span class='text-[#E91E63] dark:text-pink-400'>+2</span>Since last
          month
        </p>
      </div>

      <div
        class='rounded-[111px] bg-card px-[16px] py-[20px] text-card-foreground shadow-sm'
        style='box-shadow: rgba(0, 0, 0, 0.15) 0px 1px 2px 0px;'
      >
        <div class='flex items-center justify-between px-[10px] py-[4px]'>
          <div class='flex flex-col gap-[4px]'>
            <h3
              class='text-[14px] font-[500] text-gray-500 dark:text-gray-400'
            >Total Revenue</h3>
            <p
              class='text-[18px] font-[600] text-gray-900 dark:text-gray-100'
            >${{revenue}}</p>
          </div>
          <div>
            <div class='w-11 h-11 bg-green-50 rounded-lg flex items-center justify-center'>{{lucide 'DollarSign' 'w-5 h-5 fill-green-500'}}</div>
          </div>
        </div>
        <p
          class='flex items-center gap-[4px] text-[13px] font-[500] text-gray-500 dark:text-gray-400'
        >
          <svg
            xmlns='http://www.w3.org/2000/svg'
            width='24'
            height='20'
            viewBox='0 0 24 20'
            fill='none'
          >
            <path
              d='M7.02979 8.30925H11.0298V15.7426L13.0398 15.7676V8.30925H17.0298L12.0298 4.14258L7.02979 8.30925Z'
              fill='#4CAF50'
            ></path>
          </svg>
           <span class='text-[#4CAF50] dark:text-green-400'>+8.5%</span>Since last
           month
         </p>
       </div>

       <div
        class='rounded-[11px] bg-card px-[16px] py-[20px] text-card-foreground shadow-sm'
        style='box-shadow: rgba(0, 0, 0, 0.15) 0px 1px 2px 0px;'
      >
        <div class='flex items-center justify-between px-[10px] py-[4px]'>
          <div class='flex flex-col gap-[4px]'>
            <h3
              class='text-[14px] font-[500] text-gray-500 dark:text-gray-400'
            >Team Members</h3>
            <p
              class='text-[18px] font-[600] text-gray-900 dark:text-gray-100'
            >{{teamCount}}</p>
          </div>
          <div>
            <div class='w-11 h-11 bg-purple-50 rounded-lg flex items-center justify-center'>{{lucide 'Users' 'w-5 h-5 fill-purple-600'}}</div>
          </div>
        </div>
        <p
          class='flex items-center gap-[4px] text-[13px] font-[500] text-gray-500 dark:text-gray-400'
        >
          <svg
            xmlns='http://www.w3.org/2000/svg'
            width='24'
            height='20'
            viewBox='0 0 24 20'
            fill='none'
          >
            <path
              d='M7.02979 8.30925H11.0298V15.7426L13.0398 15.7676V8.30925H17.0298L12.0298 4.14258L7.02979 8.30925Z'
              fill='#9C27B0'
            ></path>
          </svg>
          <span class='text-[#9C27B0] dark:text-purple-400'>+5</span>Since
          last month
        </p>
      </div>
     </div>

     <!-- Section Divider -->
     <div class='my-8 border-t border-gray-200 dark:border-gray-700'></div>

     <!-- Startup Management Section -->
     <div class='mb-8'>
      <div class='flex items-center justify-between mb-4'>
        <h3 class='text-xl font-semibold text-gray-900 dark:text-gray-100'>Startups</h3>
        <div class='flex items-center gap-2 flex-wrap sm:flex-nowrap'>
          <!-- Search Input -->
          <div class='relative'>
            <input
              type='text'
              placeholder='Search startups...'
              class='pl-9 pr-4 py-1.5 text-sm border border-gray-300 dark:border-gray-600 rounded-md bg-white dark:bg-gray-800 focus:outline-none focus:ring-1 focus:ring-blue-500 min-w-[180px]'
            />
            <div class='absolute left-2.5 top-1.5 text-gray-400'>
              <svg xmlns='http://www.w3.org/2000/svg' width='16' height='16' viewBox='0 0 24 24' fill='none' stroke='currentColor' stroke-width='2' stroke-linecap='round' stroke-linejoin='round' class='lucide lucide-search'>
                <circle cx='11' cy='11' r='8'/>
                <path d='m21 21-4.3-4.3'/>
              </svg>
            </div>
          </div>
          
          <select class='px-3 py-1.5 text-sm border border-gray-300 dark:border-gray-600 rounded-md bg-white dark:bg-gray-800 min-w-[120px]'>
            <option>All Startups</option>
            <option>Active Startups</option>
            <option>Under Review</option>
            <option>Completed</option>
          </select>
          <button class='px-3 py-1.5 text-sm bg-gray-100 dark:bg-gray-700 rounded-md hover:bg-gray-200 dark:hover:bg-gray-600'>
            View All
          </button>
        </div>
      </div>
      
      <div class='grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 gap-4'>
        {{#each startups}}
        <div class='border border-gray-200 dark:border-gray-700 rounded-lg p-4 hover:shadow-md transition-shadow'>
          <div class='flex items-start justify-between'>
            <div>
              <h4 class='font-medium text-lg'>{{this.name}}</h4>
              <p class='text-sm text-gray-500 dark:text-gray-400'>{{this.description}}</p>
              <div class='mt-2 flex items-center gap-2'>
                <span class='text-xs px-2 py-1 bg-blue-100 dark:bg-blue-900/30 text-blue-800 dark:text-blue-400 rounded-full'>{{this.status}}</span>
                <span class='text-xs px-2 py-1 bg-green-100 dark:bg-green-900/30 text-green-800 dark:text-green-400 rounded-full'>{{this.stage}}</span>
              </div>
            </div>
            <div class='flex gap-1'>
              <button class='p-1.5 rounded-md hover:bg-gray-100 dark:hover:bg-gray-700' title='View'>
                <svg xmlns='http://www.w3.org/2000/svg' width='16' height='16' viewBox='0 0 24 24' fill='none' stroke='currentColor' stroke-width='2' stroke-linecap='round' stroke-linejoin='round' class='lucide lucide-eye'>
                  <path d='M2.062 12.348a1 1 0 0 1 0-.696 10.75 10.75 0 0 1 19.876 0 1 1 0 0 1 0 .696 10.75 10.75 0 0 1-19.876 0'/>
                  <circle cx='12' cy='12' r='1'/>
                </svg>
              </button>
              <button class='p-1.5 rounded-md hover:bg-gray-100 dark:hover:bg-gray-700' title='Edit'>
                <svg xmlns='http://www.w3.org/2000/svg' width='16' height='16' viewBox='0 0 24 24' fill='none' stroke='currentColor' stroke-width='2' stroke-linecap='round' stroke-linejoin='round' class='lucide lucide-edit'>
                  <path d='M11 4H4a2 2 0 0 0-2 2v14a2 2 0 0 0 2 2h14a2 2 0 0 0 2-2v-7'/>
                  <path d='M18.5 2.5a2.121 2.121 0 0 1 3 3L12 15l-4 1 1-4Z'/>
                </svg>
              </button>
              <button class='p-1.5 rounded-md hover:bg-gray-100 dark:hover:bg-gray-700' title='More options'>
                <svg xmlns='http://www.w3.org/2000/svg' width='16' height='16' viewBox='0 0 24 24' fill='none' stroke='currentColor' stroke-width='2' stroke-linecap='round' stroke-linejoin='round' class='lucide lucide-more-horizontal'>
                  <circle cx='12' cy='12' r='1'/>
                  <circle cx='19' cy='12' r='1'/>
                  <circle cx='5' cy='12' r='1'/>
                </svg>
              </button>
            </div>
          </div>
          
          <div class='mt-4 grid grid-cols-2 gap-2'>
            <div>
              <p class='text-xs text-gray-500 dark:text-gray-400'>Projects</p>
              <p class='font-medium'>{{this.projectCount}}</p>
            </div>
            <div>
              <p class='text-xs text-gray-500 dark:text-gray-400'>Team Size</p>
              <p class='font-medium'>{{this.teamSize}}</p>
            </div>
            <div>
              <p class='text-xs text-gray-500 dark:text-gray-400'>Idea Score</p>
              <p class='font-medium'>{{this.ideaScore}}/10</p>
            </div>
            <div>
              <p class='text-xs text-gray-500 dark:text-gray-400'>Last Activity</p>
              <p class='font-medium'>{{this.lastActivity}}</p>
            </div>
          </div>
          
          <div class='mt-4 pt-4 border-t border-gray-100 dark:border-gray-700'>
            <div class='flex justify-between text-sm'>
              <span class='text-gray-500 dark:text-gray-400'>Progress</span>
              <span>{{this.progress}}%</span>
            </div>
            <div class='mt-1 w-full bg-gray-200 dark:bg-gray-700 rounded-full h-2'>
              <div class='bg-blue-600 h-2 rounded-full' style='width: {{this.progress}}%'></div>
            </div>
          </div>
          
          <!-- Role Management for this Startup -->
          <div class='mt-4 pt-4 border-t border-gray-100 dark:border-gray-700'>
            <div class='flex justify-between items-center mb-2'>
              <h5 class='text-sm font-medium text-gray-700 dark:text-gray-300'>Team Roles</h5>
              <button class='text-xs text-blue-600 dark:text-blue-400 hover:underline'>Manage</button>
            </div>
            <div class='flex -space-x-2'>
              {{#each this.teamMembers}}
                <div class='w-6 h-6 rounded-full bg-gray-300 border-2 border-white dark:border-gray-800 flex items-center justify-center text-xs font-bold text-gray-700' title='{{this.name}} - {{this.role}}'>
                  {{initial this.name}}
                </div>
              {{/each}}
              <div class='w-6 h-6 rounded-full bg-gray-200 border-2 border-white dark:border-gray-800 flex items-center justify-center text-xs text-gray-600'>
                +{{this.additionalTeamCount}}
              </div>
            </div>
          </div>
        </div>
        {{/each}}
      </div>
    </div>

    <!-- Section Divider -->
    <div class='my-8 border-t border-gray-200 dark:border-gray-700'></div>

    <!-- Idea/Project Tracking -->
    <div class='mb-8'>
      <div class='flex items-center justify-between mb-4'>
        <h3 class='text-xl font-semibold text-gray-900 dark:text-gray-100'>Idea & Project Tracking</h3>
        <div class='flex items-center gap-2 flex-wrap sm:flex-nowrap'>
          <!-- Search Input -->
          <div class='relative'>
            <input
              type='text'
              placeholder='Search ideas/projects...'
              class='pl-9 pr-4 py-1.5 text-sm border border-gray-300 dark:border-gray-600 rounded-md bg-white dark:bg-gray-800 focus:outline-none focus:ring-1 focus:ring-blue-500'
            />
            <div class='absolute left-2.5 top-1.5 text-gray-400'>
              <svg xmlns='http://www.w3.org/2000/svg' width='16' height='16' viewBox='0 0 24 24' fill='none' stroke='currentColor' stroke-width='2' stroke-linecap='round' stroke-linejoin='round' class='lucide lucide-search'>
                <circle cx='11' cy='11' r='8'/>
                <path d='m21 21-4.3-4.3'/>
              </svg>
            </div>
          </div>
          
          <div class='relative'>
            <select class='px-3 py-1.5 text-sm border border-gray-300 dark:border-gray-600 rounded-md bg-white dark:bg-gray-800 appearance-none pr-8'>
              <option>All Startups</option>
              <option>Startup A</option>
              <option>Startup B</option>
              <option>Startup C</option>
            </select>
            <div class='pointer-events-none absolute inset-y-0 right-0 flex items-center px-2 text-gray-700 dark:text-gray-300'>
              <svg class='h-4 w-4' xmlns='http://www.w3.org/2000/svg' viewBox='0 0 20 20' fill='currentColor'>
                <path fill-rule='evenodd' d='M5.293 7.293a1 1 0 011.414 0L10 10.586l3.293-3.293a1 1 0 111.414 1.414l-4 4a1 1 0 01-1.414 0l-4-4a1 1 0 010-1.414z' clip-rule='evenodd' />
              </svg>
            </div>
          </div>
          <div class='relative'>
            <select class='px-3 py-1.5 text-sm border border-gray-300 dark:border-gray-600 rounded-md bg-white dark:bg-gray-800 appearance-none pr-8'>
              <option>All Status</option>
              <option>Active</option>
              <option>In Progress</option>
              <option>Under Review</option>
              <option>Completed</option>
            </select>
            <div class='pointer-events-none absolute inset-y-0 right-0 flex items-center px-2 text-gray-700 dark:text-gray-300'>
              <svg class='h-4 w-4' xmlns='http://www.w3.org/2000/svg' viewBox='0 0 20 20' fill='currentColor'>
                <path fill-rule='evenodd' d='M5.293 7.293a1 1 0 011.414 0L10 10.586l3.293-3.293a1 1 0 111.414 1.414l-4 4a1 1 0 01-1.414 0l-4-4a1 1 0 010-1.414z' clip-rule='evenodd' />
              </svg>
            </div>
          </div>
          <div class='relative'>
            <select class='px-3 py-1.5 text-sm border border-gray-300 dark:border-gray-600 rounded-md bg-white dark:bg-gray-800 appearance-none pr-8'>
              <option>All Types</option>
              <option>Ideas</option>
              <option>Projects</option>
            </select>
            <div class='pointer-events-none absolute inset-y-0 right-0 flex items-center px-2 text-gray-700 dark:text-gray-300'>
              <svg class='h-4 w-4' xmlns='http://www.w3.org/2000/svg' viewBox='0 0 20 20' fill='currentColor'>
                <path fill-rule='evenodd' d='M5.293 7.293a1 1 0 011.414 0L10 10.586l3.293-3.293a1 1 0 111.414 1.414l-4 4a1 1 0 01-1.414 0l-4-4a1 1 0 010-1.414z' clip-rule='evenodd' />
              </svg>
            </div>
          </div>
          <button class='px-3 py-1.5 text-sm bg-gray-100 dark:bg-gray-700 rounded-md hover:bg-gray-200 dark:hover:bg-gray-600 flex items-center gap-1 export-btn'>
            <svg xmlns='http://www.w3.org/2000/svg' width='16' height='16' viewBox='0 0 24 24' fill='none' stroke='currentColor' stroke-width='2' stroke-linecap='round' stroke-linejoin='round' class='lucide lucide-download'>
              <path d='M21 15v4a2 2 0 0 1-2 2H5a2 2 0 0 1-2-2v-4'/>
              <polyline points='7 10 12 15 17 10'/>
              <line x1='12' y1='15' x2='12' y2='3'/>
            </svg>
            Export
          </button>
          <button class='px-3 py-1.5 text-sm bg-primary text-primary-foreground rounded-md hover:bg-primary/90 flex items-center gap-1'>
            <svg xmlns='http://www.w3.org/2000/svg' width='16' height='16' viewBox='0 0 24 24' fill='none' stroke='currentColor' stroke-width='2' stroke-linecap='round' stroke-linejoin='round' class='lucide lucide-plus'>
              <path d='M5 12h14'/>
              <path d='M12 5v14'/>
            </svg>
            New Project
          </button>
        </div>
      </div>

      <div class='border border-gray-200 dark:border-gray-700 rounded-lg overflow-hidden'>
        <div class='-mx-4 overflow-x-auto'>
          <table class='min-w-full text-sm'>
            <thead class='bg-gray-50 dark:bg-gray-800'>
              <tr>
                <th class='py-3 px-4 text-left font-medium text-gray-700 dark:text-gray-300'>
                  <div class='flex items-center'>
                    <input type='checkbox' class='rounded border-gray-300 text-blue-600 shadow-sm focus:border-blue-300 focus:ring focus:ring-blue-200 focus:ring-opacity-50 dark:bg-gray-700 dark:border-gray-600'>
                    <span class='ml-2'>Idea/Project</span>
                  </div>
                </th>
                <th class='py-3 px-4 text-left font-medium text-gray-700 dark:text-gray-300'>Startup</th>
                <th class='py-3 px-4 text-left font-medium text-gray-700 dark:text-gray-300'>Status</th>
                <th class='py-3 px-4 text-left font-medium text-gray-700 dark:text-gray-300'>Progress</th>
                <th class='py-3 px-4 text-left font-medium text-gray-700 dark:text-gray-300'>Team</th>
                <th class='py-3 px-4 text-left font-medium text-gray-700 dark:text-gray-300'>Due Date</th>
                <th class='py-3 px-4 text-left font-medium text-gray-700 dark:text-gray-300'>Score</th>
                <th class='py-3 px-4 text-left font-medium text-gray-700 dark:text-gray-300'>Actions</th>
              </tr>
            </thead>
            <tbody class='divide-y divide-gray-200 dark:divide-gray-700'>
              {{#each projects}}
              <tr class='hover:bg-gray-50 dark:hover:bg-gray-800/50'>
                <td class='py-3 px-4'>
                  <div class='flex items-center'>
                    <input type='checkbox' class='rounded border-gray-300 text-blue-600 shadow-sm focus:border-blue-300 focus:ring focus:ring-blue-200 focus:ring-opacity-50 dark:bg-gray-700 dark:border-gray-600'>
                    <div class='ml-2 w-3 h-3 rounded-full {{#if (eq this.type 'idea')}}bg-purple-500{{else}}bg-blue-500{{/if}} mr-2'></div>
                    <div>
                      <div class='font-medium'>{{this.name}}</div>
                      <div class='text-xs text-gray-500 dark:text-gray-400'>{{this.type}}</div>
                    </div>
                  </div>
                </td>
                <td class='py-3 px-4'>
                  <span class='text-gray-700 dark:text-gray-300'>{{this.startupName}}</span>
                </td>
                <td class='py-3 px-4'>
                  <span class='px-2 py-1 text-xs rounded-full {{#if (eq this.status 'Active')}}bg-green-100 dark:bg-green-900/30 text-green-800 dark:text-green-400{{/if}}{{#if (eq this.status 'In Progress')}}bg-yellow-100 dark:bg-yellow-900/30 text-yellow-800 dark:text-yellow-400{{/if}}{{#if (eq this.status 'Under Review')}}bg-orange-100 dark:bg-orange-900/30 text-orange-800 dark:text-orange-400{{/if}}{{#if (eq this.status 'Completed')}}bg-blue-100 dark:bg-blue-900/30 text-blue-800 dark:text-blue-400{{/if}}'>
                    {{this.status}}
                  </span>
                </td>
                <td class='py-3 px-4'>
                  <div class='flex items-center gap-2'>
                    <div class='w-24 bg-gray-200 dark:bg-gray-700 rounded-full h-2'>
                      <div class='bg-blue-600 h-2 rounded-full' style='width: {{this.progress}}%'></div>
                    </div>
                    <span>{{this.progress}}%</span>
                  </div>
                </td>
                <td class='py-3 px-4'>
                  <div class='flex -space-x-2'>
                    {{#each this.teamMembers}}
