          <tbody class='divide-y divide-gray-200 dark:divide-gray-700'>
            {{#each users}}
            <tr class='hover:bg-gray-50 dark:hover:bg-gray-800/50' data-user-id='{{this.id}}'>
              <td class='py-3 px-4'>
                <div class='flex items-center'>
                  <input type='checkbox' class='rounded border-gray-300 text-blue-600 shadow-sm focus:border-blue-300 focus:ring focus:ring-blue-200 focus:ring-opacity-50 dark:bg-gray-700 dark:border-gray-600 mr-2'>
                  <div class='w-8 h-8 rounded-full bg-blue-500 flex items-center justify-center text-white text-sm font-bold mr-2'>
                    {{initial this.name}}
                  </div>
                  <div>
                    <div class='font-medium'>{{this.name}}</div>
                    <div class='text-xs text-gray-500 dark:text-gray-400'>{{this.email}}</div>
                  </div>
                </div>
              </td>
              <td class='py-3 px-4'>
                <select class='px-2 py-1 text-xs border border-gray-300 dark:border-gray-600 rounded bg-white dark:bg-gray-800 role-select' data-user-id='{{this.id}}'>
                  <option value='Admin' {{#if (eq this.role 'Admin')}}selected{{/if}}>Admin</option>
                  <option value='Manager' {{#if (eq this.role 'Manager')}}selected{{/if}}>Manager</option>
                  <option value='Editor' {{#if (eq this.role 'Editor')}}selected{{/if}}>Editor</option>
                  <option value='Viewer' {{#if (eq this.role 'Viewer')}}selected{{/if}}>Viewer</option>
                </select>
              </td>
              <td class='py-3 px-4'>
                <div class='flex flex-wrap gap-1'>
                  {{#each this.permissions}}
                    <span class='px-2 py-0.5 text-xs bg-gray-100 dark:bg-gray-700 rounded'>{{this}}</span>
                  {{/each}}
                </div>
              </td>
              <td class='py-3 px-4'>
                <div class='flex -space-x-2'>
                  {{#each this.associatedStartups}}
                    <div class='w-6 h-6 rounded-full bg-gray-300 border-2 border-white dark:border-gray-800 flex items-center justify-center text-xs font-bold text-gray-700' title='{{this}}'>
                      {{initial this}}
                    </div>
                  {{/each}}
                  {{#if this.additionalStartupsCount}}
                    <div class='w-6 h-6 rounded-full bg-gray-200 border-2 border-white dark:border-gray-800 flex items-center justify-center text-xs text-gray-600'>
                      +{{this.additionalStartupsCount}}
                    </div>
                  {{/if}}
                </div>
              </td>
              <td class='py-3 px-4'>
                <span class='px-2 py-1 text-xs rounded-full {{#if this.active}}bg-green-100 dark:bg-green-900/30 text-green-800 dark:text-green-400{{else}}bg-gray-100 dark:bg-gray-700 text-gray-800 dark:text-gray-300{{/if}}'>
                  {{#if this.active}}Active{{else}}Inactive{{/if}}
                </span>
              </td>
              <td class='py-3 px-4 text-gray-700 dark:text-gray-300'>{{this.lastLogin}}</td>
              <td class='py-3 px-4'>
                <div class='flex gap-1'>
                  <button class='p-1.5 rounded-md hover:bg-gray-100 dark:hover:bg-gray-700 edit-permissions-btn' data-user-id='{{this.id}}' title='Edit Permissions'>
                    <svg xmlns='http://www.w3.org/2000/svg' width='14' height='14' viewBox='0 0 24 24' fill='none' stroke='currentColor' stroke-width='2' stroke-linecap='round' stroke-linejoin='round' class='lucide lucide-settings'>
                      <circle cx='12' cy='12' r='3'/>
                      <path d='M19.4 15a1.65 1.65 0 0 0 .33 1.82l.06.06a2 2 0 0 1 0 2.83 2 2 0 0 1-2.83 0l-.06-.06a1.65 1.65 0 0 0-1.82-.33 1.65 1.65 0 0 0-1 1.51V21a2 2 0 0 1-2 2 2 2 0 0 1-2-2v-.09A1.65 1.65 0 0 0 9 19.4a1.65 1.65 0 0 0-1.82.33l-.06.06a2 2 0 0 1-2.83 0 2 2 0 0 1 0-2.83l.06-.06a1.65 1.65 0 0 0 .33-1.82 1.65 1.65 0 0 0-1.51-1H3a2 2 0 0 1-2-2 2 2 0 0 1 2-2h.09A1.65 1.65 0 0 0 4.6 9a1.65 1.65 0 0 0-.33-1.82l-.06-.06a2 2 0 0 1 0-2.83 2 2 0 0 1 2.83 0l.06.06a1.65 1.65 0 0 0 1.82.33H9a1.65 1.65 0 0 0 1-1.51V3a2 2 0 0 1 2-2 2 2 0 0 1 2 2v.09a1.65 1.65 0 0 0 1 1.51 1.65 1.65 0 0 0 1.82-.33l.06-.06a2 2 0 0 1 2.83 0 2 2 0 0 1 0 2.83l-.06.06a1.65 1.65 0 0 0-.33 1.82V9a1.65 1.65 0 0 0 1.51 1H21a2 2 0 0 1 2 2 2 2 0 0 1-2 2h-.09a1.65 1.65 0 0 0-1.51 1z'/>
                    </svg>
                  </button>
                  <button class='p-1.5 rounded-md hover:bg-gray-100 dark:hover:bg-gray-700' title='More options'>
                    <svg xmlns='http://www.w3.org/2000/svg' width='14' height='14' viewBox='0 0 24 24' fill='none' stroke='currentColor' stroke-width='2' stroke-linecap='round' stroke-linejoin='round' class='lucide lucide-more-horizontal'>
                      <circle cx='12' cy='12' r='1'/>
                      <circle cx='19' cy='12' r='1'/>
                      <circle cx='5' cy='12' r='1'/>
                    </svg>
                  </button>
                </div>
              </td>
            </tr>
            {{/each}}
          </tbody>
        </table>
      </div>
      
      <!-- Permissions Modal (Hidden by default) -->
      <div id='permissions-modal' class='fixed inset-0 bg-black bg-opacity-50 flex items-center justify-center hidden z-50 p-4'>
        <div class='bg-white dark:bg-gray-800 rounded-lg w-full max-w-md'>
          <div class='p-4 border-b border-gray-200 dark:border-gray-700 flex justify-between items-center'>
            <h3 class='text-lg font-semibold'>Edit Permissions</h3>
            <button id='close-permissions-modal' class='text-gray-500 hover:text-gray-700 dark:text-gray-400 dark:hover:text-gray-200'>
              <svg xmlns='http://www.w3.org/2000/svg' width='20' height='20' viewBox='0 0 24 24' fill='none' stroke='currentColor' stroke-width='2' stroke-linecap='round' stroke-linejoin='round' class='lucide lucide-x'>
                <path d='M18 6 6 18'/>
                <path d='m6 6 12 12'/>
              </svg>
            </button>
          </div>
          <div class='p-4'>
            <div class='mb-4'>
              <label class='block text-sm font-medium text-gray-700 dark:text-gray-300 mb-2'>User</label>
              <div id='modal-user-name' class='text-gray-900 dark:text-gray-100 font-medium'></div>
            </div>
            
            <div class='mb-4'>
              <label class='block text-sm font-medium text-gray-700 dark:text-gray-300 mb-2'>Role</label>
              <select id='modal-user-role' class='w-full px-3 py-2 border border-gray-300 dark:border-gray-600 rounded-md bg-white dark:bg-gray-700'>
                <option value='Admin'>Admin</option>
                <option value='Manager'>Manager</option>
                <option value='Editor'>Editor</option>
                <option value='Viewer'>Viewer</option>
              </select>
            </div>
            
            <div class='mb-4'>
              <label class='block text-sm font-medium text-gray-700 dark:text-gray-300 mb-2'>Permissions</label>
              <div class='space-y-2' id='permissions-checklist'>
                <label class='flex items-center'>
                  <input type='checkbox' value='read' class='rounded border-gray-300 text-blue-600 shadow-sm focus:border-blue-300 focus:ring focus:ring-blue-200 focus:ring-opacity-50 dark:bg-gray-700 dark:border-gray-600'>
                  <span class='ml-2 text-sm text-gray-700 dark:text-gray-300'>Read</span>
                </label>
                <label class='flex items-center'>
                  <input type='checkbox' value='write' class='rounded border-gray-300 text-blue-600 shadow-sm focus:border-blue-300 focus:ring focus:ring-blue-200 focus:ring-opacity-50 dark:bg-gray-700 dark:border-gray-600'>
                  <span class='ml-2 text-sm text-gray-700 dark:text-gray-300'>Write</span>
                </label>
                <label class='flex items-center'>
                  <input type='checkbox' value='delete' class='rounded border-gray-300 text-blue-600 shadow-sm focus:border-blue-300 focus:ring focus:ring-blue-200 focus:ring-opacity-50 dark:bg-gray-700 dark:border-gray-600'>
                  <span class='ml-2 text-sm text-gray-700 dark:text-gray-300'>Delete</span>
                </label>
                <label class='flex items-center'>
                  <input type='checkbox' value='admin' class='rounded border-gray-300 text-blue-600 shadow-sm focus:border-blue-300 focus:ring focus:ring-blue-200 focus:ring-opacity-50 dark:bg-gray-700 dark:border-gray-600'>
                  <span class='ml-2 text-sm text-gray-700 dark:text-gray-300'>Administrate</span>
                </label>
              </div>
            </div>
          </div>
          <div class='p-4 border-t border-gray-200 dark:border-gray-700 flex justify-end gap-2'>
            <button id='cancel-permissions' class='px-4 py-2 text-sm border border-gray-300 dark:border-gray-600 rounded-md hover:bg-gray-50 dark:hover:bg-gray-700'>
              Cancel
            </button>
            <button id='save-permissions' class='px-4 py-2 text-sm bg-primary text-primary-foreground rounded-md hover:bg-primary/90'>
              Save
            </button>
          </div>
        </div>
      </div>
    </div>
    
    <!-- Data Export Modal -->
    <div id='export-modal' class='fixed inset-0 bg-black bg-opacity-50 flex items-center justify-center hidden z-50 p-4'>
      <div class='bg-white dark:bg-gray-800 rounded-lg w-full max-w-md'>
        <div class='p-4 border-b border-gray-200 dark:border-gray-700 flex justify-between items-center'>
          <h3 class='text-lg font-semibold'>Export Data</h3>
          <button id='close-export-modal' class='text-gray-500 hover:text-gray-700 dark:text-gray-400 dark:hover:text-gray-200'>
            <svg xmlns='http://www.w3.org/2000/svg' width='20' height='20' viewBox='0 0 24 24' fill='none' stroke='currentColor' stroke-width='2' stroke-linecap='round' stroke-linejoin='round' class='lucide lucide-x'>
              <path d='M18 6 6 18'/>
              <path d='m6 6 12 12'/>
            </svg>
          </button>
        </div>
        <div class='p-4'>
          <div class='mb-4'>
            <label class='block text-sm font-medium text-gray-700 dark:text-gray-300 mb-2'>Export Section</label>
            <select id='export-section' class='w-full px-3 py-2 border border-gray-300 dark:border-gray-600 rounded-md bg-white dark:bg-gray-700'>
              <option value='startups'>Startups</option>
              <option value='projects'>Projects & Ideas</option>
              <option value='users'>Users & Roles</option>
              <option value='analytics'>Analytics Data</option>
              <option value='all'>All Data</option>
            </select>
          </div>
          
          <div class='mb-4'>
            <label class='block text-sm font-medium text-gray-700 dark:text-gray-300 mb-2'>Format</label>
            <div class='grid grid-cols-3 gap-2' id='format-options'>
              <label class='flex items-center justify-center p-2 border border-gray-300 dark:border-gray-600 rounded-md cursor-pointer hover:bg-gray-50 dark:hover:bg-gray-700'>
                <input type='radio' name='format' value='csv' class='mr-2' checked>
                <span class='text-sm'>CSV</span>
              </label>
              <label class='flex items-center justify-center p-2 border border-gray-300 dark:border-gray-600 rounded-md cursor-pointer hover:bg-gray-50 dark:hover:bg-gray-700'>
                <input type='radio' name='format' value='excel' class='mr-2'>
                <span class='text-sm'>Excel</span>
              </label>
              <label class='flex items-center justify-center p-2 border border-gray-300 dark:border-gray-600 rounded-md cursor-pointer hover:bg-gray-50 dark:hover:bg-gray-700'>
                <input type='radio' name='format' value='pdf' class='mr-2'>
                <span class='text-sm'>PDF</span>
              </label>
            </div>
          </div>
          
          <div class='mb-4'>
            <label class='flex items-center'>
              <input type='checkbox' id='include-filters' class='rounded border-gray-300 text-blue-600 shadow-sm focus:border-blue-300 focus:ring focus:ring-blue-200 focus:ring-opacity-50 dark:bg-gray-700 dark:border-gray-600'>
              <span class='ml-2 text-sm text-gray-700 dark:text-gray-300'>Include current filters</span>
            </label>
          </div>
        </div>
        <div class='p-4 border-t border-gray-200 dark:border-gray-700 flex justify-end gap-2'>
          <button id='cancel-export' class='px-4 py-2 text-sm border border-gray-300 dark:border-gray-600 rounded-md hover:bg-gray-50 dark:hover:bg-gray-700'>
            Cancel
          </button>
          <button id='start-export' class='px-4 py-2 text-sm bg-primary text-primary-foreground rounded-md hover:bg-primary/90 flex items-center gap-2'>
            <svg xmlns='http://www.w3.org/2000/svg' width='16' height='16' viewBox='0 0 24 24' fill='none' stroke='currentColor' stroke-width='2' stroke-linecap='round' stroke-linejoin='round' class='lucide lucide-download'>
              <path d='M21 15v4a2 2 0 0 1-2 2H5a2 2 0 0 1-2-2v-4'/>
              <polyline points='7 10 12 15 17 10'/>
              <line x1='12' y1='15' x2='12' y2='3'/>
            </svg>
            Export
          </button>
        </div>
      </div>
    </div>
  </div>
</div>

<script>
  // Function to handle startup selection and filtering
  function initEnterpriseDashboard() {
    // Handle new startup button
    const newStartupBtn = document.querySelector('button[title="New Startup"]');
    if (newStartupBtn) {
      newStartupBtn.addEventListener('click', function() {
        // Open startup creation modal or navigate to creation page
        console.log('Creating new startup...');
        // In a real implementation, this would open a modal or navigate to a form
      });
    }

    // Handle startup management actions
    const startupEditBtns = document.querySelectorAll('.startup-edit-btn');
    startupEditBtns.forEach(btn => {
      btn.addEventListener('click', function() {
        const startupId = this.dataset.startupId;
        console.log(`Editing startup: ${startupId}`);
        // In a real implementation, this would open an edit form
      });
    });

    // Initialize data export functionality
    initExportFunctionality();
    
    // Initialize permissions modal
    initPermissionsModal();
    
    // Initialize charts if available
    initCharts();
  }

  // Initialize data export functionality
  function initExportFunctionality() {
    const exportButtons = document.querySelectorAll('.export-btn, [id*="export"], [class*="export"]');
    
    // Add event listeners to all export buttons
    exportButtons.forEach(button => {
      button.addEventListener('click', function() {
        document.getElementById('export-modal').classList.remove('hidden');
      });
    });
    
    // Close export modal
    document.getElementById('close-export-modal').addEventListener('click', function() {
      document.getElementById('export-modal').classList.add('hidden');
    });
    
    document.getElementById('cancel-export').addEventListener('click', function() {
      document.getElementById('export-modal').classList.add('hidden');
    });
    
    // Handle export start
    document.getElementById('start-export').addEventListener('click', function() {
      const section = document.getElementById('export-section').value;
      const format = document.querySelector('input[name="format"]:checked').value;
      const includeFilters = document.getElementById('include-filters').checked;
      
      console.log(`Exporting ${section} data in ${format} format. Include filters: ${includeFilters}`);
      
      // In a real implementation, this would trigger the actual export
      // For now, just show a success message and close the modal
      alert(`Export started: ${section} data as ${format.toUpperCase()}`);
      document.getElementById('export-modal').classList.add('hidden');
    });
  }
  
  // Initialize permissions modal
  function initPermissionsModal() {
    // Handle edit permissions buttons
    document.querySelectorAll('.edit-permissions-btn').forEach(button => {
      button.addEventListener('click', function() {
        const userId = this.dataset.userId;
        const userRow = document.querySelector(`tr[data-user-id="${userId}"]`);
        const userName = userRow.querySelector('td:first-child .font-medium').textContent;
        const userRole = userRow.querySelector('.role-select').value;
        
        document.getElementById('modal-user-name').textContent = userName;
        document.getElementById('modal-user-role').value = userRole;
        
        // In a real implementation, we would load the user's permissions here
        // For now, just check some sample permissions based on role
        const permissionsChecklist = document.getElementById('permissions-checklist');
        const checkboxes = permissionsChecklist.querySelectorAll('input[type="checkbox"]');
        checkboxes.forEach(checkbox => {
          // Reset all
          checkbox.checked = false;
          
          // Set based on role
          if (userRole === 'Admin') {
            checkbox.checked = true;
          } else if (userRole === 'Manager') {
            if (checkbox.value === 'read' || checkbox.value === 'write') {
              checkbox.checked = true;
            }
          } else if (userRole === 'Editor') {
            if (checkbox.value === 'read') {
              checkbox.checked = true;
            }
          }
        });
        
        document.getElementById('permissions-modal').classList.remove('hidden');
      });
    });
    
    // Close permissions modal
    document.getElementById('close-permissions-modal').addEventListener('click', function() {
      document.getElementById('permissions-modal').classList.add('hidden');
    });
    
    document.getElementById('cancel-permissions').addEventListener('click', function() {
      document.getElementById('permissions-modal').classList.add('hidden');
    });
    
    // Handle save permissions
    document.getElementById('save-permissions').addEventListener('click', function() {
      const userId = document.querySelector('tr[data-user-id]').dataset.userId; // This would be the current user being edited
      const newRole = document.getElementById('modal-user-role').value;
      const permissions = Array.from(document.querySelectorAll('#permissions-checklist input:checked'))
        .map(checkbox => checkbox.value);
      
      console.log(`Updating user ${userId} to role ${newRole} with permissions:`, permissions);
      
      // In a real implementation, this would update the user's permissions
      // For now, just show a success message and close the modal
      alert(`Permissions updated for user. Role: ${newRole}, Permissions: ${permissions.join(', ')}`);
      document.getElementById('permissions-modal').classList.add('hidden');
    });
  }

  // Initialize charts if available
  function initCharts() {
    console.log('Initializing charts...');
    // In a real implementation, this would initialize charting libraries
  }

  // Initialize when page loads
  document.addEventListener('DOMContentLoaded', initEnterpriseDashboard);
</script>